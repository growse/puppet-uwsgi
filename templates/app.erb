# <%= @name %>
#
# This uwsgi configuration is managed by Puppet.
# Do not edit this file. Your changes will be lost.
[uwsgi]
<% if @uid -%>
    uid = <%= @uid %>
<% end -%>
<% if @gid -%>
    gid = <%= @gid %>
<% end -%>
    vhost = <%= @vhost %>
    plugins = <%=plugins.join(',') %>
<% if @socket -%>
    socket = <%= @socket %>
<% end -%>
<% if @http_socket -%>
    http-socket = <%= @http_socket %>
<% end -%>
<% if @master -%>
    master = <%= @master %>
<% end -%>
<% if @enable_threads -%>
    enable-threads = <%= @enable_threads %>
<% end -%>
    processes = <%= @processes %>
<% if @env -%>
    env = <%= @env %>
<% end -%>
<% if @pythonpath -%>
    pythonpath = <%= @pythonpath %>
<% end -%>
<% if @wsgi_file -%>
    wsgi-file = <%= @wsgi_file %>
<% end -%>
<% if @touch_reload -%>
    touch-reload = <%= @touch_reload %>
<% end -%>
<% if @virtualenv -%>
    virtualenv = <%= @virtualenv %>
<% end -%>
<% if @chdir -%>
    chdir = <%= @chdir %>
<% end -%>
<% if @modulename -%>
    module = <%= @modulename %>
<% end -%>
<% if @home -%>
    home = <%= @home %>
<% end -%>
<% if @buffersize -%>
    buffer-size = <%= buffersize %>
<% end -%>
